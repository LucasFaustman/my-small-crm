<!doctype html>
<html lang="en" data-layout="vertical" data-topbar="light" data-sidebar="dark" data-sidebar-size="lg" data-sidebar-image="none">

<head>

    <meta charset="utf-8" />
    <title> <%-title%> | My Small CRM</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="My Small CRM - The CRM for small businesses" name="description" />
    <meta content="Themesbrand" name="author" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="/assets/images/logos/vector/default-monochrome-white.svg">
    <%- HeaderCss %>

<%- include ('./head-css.ejs') %>

</head>

<body>
    <div id="layout-wrapper">
        <%- include ('./topbar') %>
        <%- include ('./sidebar') %>
        <div class="main-content">
            <div class="page-content">
                <div class="container-fluid">
                    <%- include ('./page-title') %>
                    <%- body%>
                </div>
                <!-- container-fluid -->
            </div>
            <!-- End Page-content -->
            <%- include ('./footer') %>

        </div>
        <!-- end main content-->
    </div>
    <!-- END layout-wrapper -->
    <%- include ('./customizer') %>
    <%- include ('./vendor-scripts') %>
    <%- FooterJs %>

    <!-- App js -->
    <script src="/assets/js/app.js"></script>
</body>

</html>